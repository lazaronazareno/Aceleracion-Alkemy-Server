{
  "name": "ong-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "nodemon ./bin/www",
    "lint": "eslint ./",
    "lint:fix": "eslint ./ --fix"
  },
  "engines": {
    "node": ">=0.14"
  },
  "husky": {
    "hooks": {
        "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
        "eslint --quiet --fix"
    ],
    "*.{json,md,html}": [
        "prettier --write"
    ]
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "express-validator": "^6.12.1",
    "http-errors": "~1.6.3",
    "log4js": "^6.3.0",
    "morgan": "~1.9.1",
    "mysql2": "^2.2.3",
    "sequelize": "^6.3.5"
  },
  "devDependencies": {
    "eslint": "^7.31.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.1",
    "prettier": "^2.3.2",
    "sequelize-cli": "^6.2.0"
  }
}
